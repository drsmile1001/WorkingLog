@model object

<h1>上傳工作日誌</h1>
<form method="post" enctype="multipart/form-data" asp-controller="Home" asp-action="Upload">
    <div class="form-group">
        <div class="row">
            <div class="col-md-10">
                <input class="form-control" type="file" name="file"/>
            </div>
            <div class="col-md-2">
                <input class="btn btn-default btn-block" type="submit" value="上傳"/>
            </div>
        </div>
    </div>
</form>